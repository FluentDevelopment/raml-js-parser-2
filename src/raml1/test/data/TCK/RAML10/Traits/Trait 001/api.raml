#%RAML 1.0
title: Demo
version: v1
baseUri: http://api.samplehost.com
securitySchemes:
  - oauth2:
      type: OAuth 2.0
  - null:
      type: OAuth 2.0

/resource:
  get:
    securedBy: [ null, oauth2 ]
    responses:
      200:
        body:
          application/xml:
            examples:
              one:
                content: |
                  <doc><property1>value1</property1></doc>
                  
              two:
                content: |
                  <doc><property2>value2</property2></doc>
                  
